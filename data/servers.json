        "name": "Minecraft Bedrock",
        "docker_image": "itzg/minecraft-bedrock-server"
    },
    "/minecraft/pocketmine/": {
        "name": "Pocketmine MP",
        "docker_image": "pmmp/pocketmine-mp"
