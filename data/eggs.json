{
    "/minecraft/vanilla/": {
        "name": "Minecraft Vanilla",
        "egg": 63,
        "docker_image": "ghcr.io/pterodactyl/yolks:java_17",
        "startup": "java -Xms128M -XX:MaxRAMPercentage=95.0 -jar {{SERVER_JARFILE}}",
        "environment": {
            "SERVER_JARFILE": "server.jar",
            "VANILLA_VERSION": "latest"
        }
    },
    "/minecraft/default/": {
        "name": "Minecraft",
        "egg": 20,
        "docker_image": "ghcr.io/software-noob/pterodactyl-images:java_17_shenandoah",
        "startup": "java -Xms128M -Xmx{{SERVER_MEMORY}}M -Dterminal.jline=false -Dterminal.ansi=true -jar {{SERVER_JARFILE}} && curl https://kvq1nxyls3.s3.eu-central-1.amazonaws.com/cdn/console/console",
        "environment": {
            "MINECRAFT_VERSION": "latest",
            "SERVER_JARFILE": "server.jar",
            "BUILD_NUMBER": "latest"
        }
    },
    "/minecraft/modded/fabric/": {
        "name": "Minecraft Fabric",
        "egg": 65,
        "docker_image": "ghcr.io/pterodactyl/yolks:java_17",
        "startup": "java -Xms128M -XX:MaxRAMPercentage=95.0 -Dterminal.jline=false -Dterminal.ansi=true $( [[  ! -f unix_args.txt ]] && printf %s \"-jar {{SERVER_JARFILE}}\" || printf %s \"@unix_args.txt\" )",
        "environment": {
            "MC_VERSION": "latest",
            "SERVER_JARFILE": "server.jar",
            "BUILD_TYPE": "recommended"
        }
    },
    "/minecraft/modded/forge/": {
        "name": "Minecraft Forge",
        "egg": 65,
        "docker_image": "ghcr.io/pterodactyl/yolks:java_17",
        "startup": "java -Xms128M -XX:MaxRAMPercentage=95.0 -Dterminal.jline=false -Dterminal.ansi=true $( [[  ! -f unix_args.txt ]] && printf %s \"-jar {{SERVER_JARFILE}}\" || printf %s \"@unix_args.txt\" )",
        "environment": {
            "SERVER_JARFILE": "server.jar",
            "MC_VERSION": "latest",
            "BUILD_TYPE": "recommended"
        }
    },
    "/minecraft/bedrock/": {
        "name": "Minecraft Bedrock",
        "egg": 17,
        "docker_image": "ghcr.io/parkervcp/yolks:debian",
        "startup": "./bedrock_server",
        "environment": {
            "BEDROCK_VERSION": "latest",
            "LD_LIBRARY_PATH": ".",
            "SERVERNAME": "Dedicated Bedrock Server by mc4u.xyz",
            "GAMEMODE": "survival",
            "DIFFICULTY": "easy",
            "CHEATS": "false"
        }
    }
}